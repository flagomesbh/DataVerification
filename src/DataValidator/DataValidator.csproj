<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Lucas Mundim, Andre Minellil, Gabriel Santos, Tulio Rezende, Arthur Iperoyg, Andre Borba</Authors>
    <Company>Take</Company>
    <Description>NuGet package to validate formats of different identifier strings such as CPF, CNPJ, phone numbers, car plates...</Description>
    <PackageProjectUrl>https://lfmundim.github.io/DataVerification/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/lfmundim/DataVerification</RepositoryUrl>
    <PackageTags>verification, validation, cpf, cnpj, plate, phone</PackageTags>
    <Version>1.0.2</Version>
    <PackageReleaseNotes>Added plate checker with mercosul and brazilian standards</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\DataValidator.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\DataValidator.xml</DocumentationFile>
  </PropertyGroup>

</Project>
